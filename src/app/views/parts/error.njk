{% extends "layout.njk" %}
{% import "parts/card.njk" as cards %}

{% block content %}

{% call cards.error('Erro ao realizar o cadastro!') %}

    <a href="#" onclick="history.back()">Tente novamente</a>
    
{% endcall %}

<script src="/lottie.min.js" type="text/javascript"></script>
<script>
    const element = document.querySelector('.error-lottie')
    lottie.loadAnimation({
    container: element, // the dom element that will contain the animation
    renderer: 'svg',
    loop: false,
    autoplay: true,
    path: '/5707-error.json' // the path to the animation json
    });
</script>

{% endblock %}